<% vals =  Array(generic_file[key]) %>
<tr>
  <th><%= get_label(key) %></th>
  <td>
    <% vals.each do |v| %>
      <span itemprop="name">
        <%= link_function.call v unless local_assigns.has_key? :opt_param %>
        <%= link_function.call v, opt_param if local_assigns.has_key? :opt_param %>
      </span>
        <br />
    <% end %>
  </td>
</tr>